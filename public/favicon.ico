  <circle cx="25" cy="8" r="2" fill="#374151" opacity="0.4"/>
  <circle cx="24.5" cy="7.5" r="0.3" fill="#6B7280" opacity="0.6"/>
  
  <!-- Cat ears (positioned above the letters) -->
  <path d="M10 6 L12 3 L14 6 Z" fill="#6EE7B7" opacity="0.9"/>
  <path d="M18 6 L20 3 L22 6 Z" fill="#6EE7B7" opacity="0.9"/>
  
